{
    "name": "Source Tepthon Arabic",
    "description": "Deploy Source Tepthon Arabic on Scalingo for free",
    "addons": ["scalingo-postgresql"],
    "keywords": ["tepthon", "userbot", "scalingo"],
    "repository": "https://github.com/RRYR7/scalingo-deploy",
    "logo": "https://graph.org/file/9a2e99d95903e6b8f9fb9.jpg",
    "stack": "container",
    "env": {
        "STRING_SESSION": {
            "description": "Termux session code - the large code from your account storage or from the bot T66bot@",
            "required": true
        },
        "TG_BOT_TOKEN": {
            "description": "Bot token - obtain it from BotFather @BotFather",
            "required": true,
            "value": ""
        },
        "APP_ID": {
            "description": "App ID code",
            "required": true,
            "value": ""
        },
        "API_HASH": {
            "description": "API Hash code, leave it as is",
            "required": true,
            "value": ""
        },
        "TZ": {
            "description": "Time zone, do not change",
            "required": true,
            "value": "Asia/Kolkata"
        },
        "ENV": {
            "description": "Do not change this",
            "required": true,
            "value": "ANYTHING"
        }
    }
}
